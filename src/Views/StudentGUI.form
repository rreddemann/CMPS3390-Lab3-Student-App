          </vspacer>
        </children>
      </grid>
      <grid id="8b164" binding="studentActions" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
        <constraints border-constraint="South"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="c66b8" class="javax.swing.JButton" binding="buttonAddStudent">
            <constraints/>
            <properties>
              <text value="Add Student"/>
            </properties>
          </component>
          <component id="2cf67" class="javax.swing.JButton" binding="buttonRemoveStudent">
            <constraints/>
            <properties>
              <text value="Remove Student"/>
            </properties>
          </component>
        </children>
      </grid>
    </children>
  </grid>
</form>
