              <gridbag top="5" left="5" bottom="5" right="5" weightx="0.0" weighty="0.0" ipadx="5" ipady="5"/>
            </constraints>
            <properties>
              <columns value="10"/>
            </properties>
          </component>
          <component id="993c4" class="javax.swing.JLabel" binding="labelLastName">
            <constraints>
              <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
              <gridbag top="5" left="5" bottom="5" right="5" weightx="0.0" weighty="0.0"/>
            </constraints>
            <properties>
              <text value="Last Name"/>
            </properties>
          </component>
          <component id="d947a" class="javax.swing.JTextField" binding="inputLastName">
            <constraints>
              <grid row="1" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
                <preferred-size width="150" height="-1"/>
              </grid>
              <gridbag top="5" left="5" bottom="5" right="5" weightx="0.0" weighty="0.0" ipadx="5" ipady="5"/>
            </constraints>
            <properties/>
          </component>
          <component id="2f1de" class="javax.swing.JLabel" binding="labelStudentID">
            <constraints>
              <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
              <gridbag top="5" left="5" bottom="5" right="5" weightx="0.0" weighty="0.0"/>
            </constraints>
            <properties>
              <text value="Student ID"/>
            </properties>
          </component>
          <component id="7d14d" class="javax.swing.JTextField" binding="inputStudentID">
            <constraints>
              <grid row="2" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
                <preferred-size width="150" height="-1"/>
              </grid>
              <gridbag top="5" left="5" bottom="5" right="5" weightx="0.0" weighty="0.0" ipadx="5" ipady="5"/>
            </constraints>
            <properties>
              <columns value="15"/>
            </properties>
          </component>
          <vspacer id="32abb">
            <constraints>
              <grid row="3" column="1" row-span="1" col-span="1" vsize-policy="6" hsize-policy="1" anchor="0" fill="2" indent="0" use-parent-layout="false"/>
              <gridbag weightx="0.0" weighty="10.0"/>
